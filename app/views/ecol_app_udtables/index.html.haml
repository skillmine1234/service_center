%br
%h1 Listing E-Collect App UDTable
%br
= will_paginate @records, :page_links => false
%br
%table.table.table-bordered.table-striped.table-hover
  .thead
    %th{:style=>'text-align:left; background-color: lightblue;'}
    %th{:style=>'text-align:left; background-color: lightblue;'}
    %th{:style=>'text-align:left; background-color: lightblue;'}
      App Code
    %th{:style=>'text-align:left; background-color: lightblue;'}
      UDF1
    %th{:style=>'text-align:left; background-color: lightblue;'}
      UDF2
    %th{:style=>'text-align:left; background-color: lightblue;'}
      UDF3
    %th{:style=>'text-align:left; background-color: lightblue;'}
      UDF4
    %th{:style=>'text-align:left; background-color: lightblue;'}
      UDF5

  .tbody
    - @records.each do |ecol_app_udtable|
      %tr
        %td{:style=>'text-align:left;'}
          = link_to 'Show', ecol_app_udtable
        %td{:style=>'text-align:left;'}
          = link_to 'Edit', edit_ecol_app_udtable_path(ecol_app_udtable)
        %td{:style=>'text-align:left;'}
          = ecol_app_udtable.app_code
        %td{:style=>'text-align:left;'}
          = ecol_app_udtable.udf1
        %td{:style=>'text-align:left;'}
          = ecol_app_udtable.udf2
        %td{:style=>'text-align:left;'}
          = ecol_app_udtable.udf3
        %td{:style=>'text-align:left;'}
          = ecol_app_udtable.udf4
        %td{:style=>'text-align:left;'}
          = ecol_app_udtable.udf5
