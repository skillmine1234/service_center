= form_tag '', :method => 'get', :class => 'form-horizontal', :autocomplete=>"off" do
  .control-group
    = label_tag "Enabled?", "Enabled?", :class => 'control-label'
    = select_tag :is_enabled, options_for_select([['Yes','Y'],['No','N']],params[:is_enabled]),:prompt => "ANY"
  .control-group
    = label_tag "Beneficiary Customer ID", "Beneficiary Customer ID", :class => 'control-label'
    = text_field_tag :code, params[:code]
  .control-group
    = label_tag "Beneficiary Account No", "Beneficiary Account No", :class => 'control-label'
    = text_field_tag :bene_account_no, params[:bene_account_no]
  .control-group
    = label_tag "Provider Account No", "Provider Account No", :class => 'control-label'
    = text_field_tag :debit_account_no, params[:debit_account_no]
  .control-group
    = label_tag "Notify Mobile No", "Notify Mobile No", :class => 'control-label'
    = text_field_tag :notify_mobile_no, params[:notify_mobile_no]
    = hidden_field_tag :approval_status, params[:approval_status]
  .form-actions
    = submit_tag "Search", :name => nil, :class => 'btn btn-primary', :id => "adv_search_button"
    = button_tag "Reset", :type => 'button', :value=>'Reset', :class => 'btn', :id => 'rc_transfer_schedule_reset'
  = hidden_field_tag :advanced_search, :true

:javascript
  var in_advanced_search = true;
